  value={
    Object {
      "slots": Object {
        "slot": Array [
          [Function],
          [Function],
        ],
      },
    }
  }
>
  <Consumer(Slot)
    name="slot"
    props={
      Object {
        "id": "Pizza",
      }
    }
  />
</ContextProvider>
`;

