{
	"parser": "babel-eslint",
	"extends": "tamia/react",
	"rules": {
		"valid-jsdoc": 0,
		"react/prefer-stateless-function": 0
	},
	"overrides": [
		{
			"files": ["*.ts", "*.tsx"],
			"parser": "@typescript-eslint/parser",
			"plugins": ["@typescript-eslint"],
			"rules": {
				"no-unused-vars": "off",
				"import/no-unresolved": "off",
				"import/extensions": "off"
			}
		}
	]
}
